<% obj.each do |artschedule| %>
    <div class="row">
      <div class="col-md-2">
        <section class ="center">
  
        </section>
      </div>
      <div class="col-md-8 well">
        <h5>Art Project Details: </h5>
         <ul>
           <li><%= artschedule.art_project.try(:medium).capitalize %></li>
           <li><%= artschedule.art_project.try(:artist_reference).capitalize %></li>
           <li><%= artschedule.art_project.try(:idea).capitalize %></li>
           <li><%= artschedule.art_project.try(:content).capitalize %></li>
           <li><%= artschedule.art_project.try(:price) %> price of <%= artschedule.art_project.try(:price) %></li>
           <li><%= artschedule.art_project.try(:day).strftime("%b %d, %Y") %></li>
         </ul>
         <h5><%= link_to "Art Schedule:", art_schedule_path(artschedule) %></h5>
         <ul>
           <li> Art Project Time: <%= artschedule.art_time %></li>
           <li> Art Project Type: <%= artschedule.art_type %></li>
         </ul>
         <p>
           <span class="quiet"></span><small>Created <%= time_ago_in_words(artschedule.created_at) %> ago</small></span>
         </p>
         <% if logged_in? && current_user.id == artschedule.art_plan.user_id %>
          <div class="artplan-actions">
            <%= link_to "Edit Art Project Schedule", edit_art_schedule_path(artschedule),  class:"btn btn-xs btn-warning" %>
            <%= link_to "Delete Art Project Schedule", art_schedule_path(artschedule), method: :delete, data: { confirm: "Are you sure you want to delete this art project schedule?"}, class:"btn btn-xs btn-danger" %>
          </div>
          <% end %>
      </div>
    </div>
  <% end %>